{
  "name": "Mute Ads",
  "version": "1.0.1",
  "description": "Mute Ads",
  "manifest_version": 3,
  "author": "",
  "action": {
    "default_popup": "index.html",
    "default_title": "Mute Ads"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "storage", "activeTab", "scripting"]
}
